# Temenos RAG API Configuration
TEMENOS_JWT_TOKEN=your_jwt_token_here
DEMO_MODE=false

# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=true
SECRET_KEY=your_secret_key_here
PORT=5000

# Database Configuration (if needed)
DATABASE_URL=sqlite:///app.db

# Azure Configuration (for deployment)
AZURE_WEBAPP_NAME=tbsg-ai-rfp-assistant
AZURE_RESOURCE_GROUP=tolis-working-rg
AZURE_CONTAINER_APP_ENV=tolis-aca-env

# Email Configuration (if needed)
MAIL_SERVER=smtp.gmail.com
MAIL_PORT=587
MAIL_USE_TLS=true
MAIL_USERNAME=your_email@gmail.com
MAIL_PASSWORD=your_app_password

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=app.log

# Docker Configuration
DOCKER_REGISTRY=ghcr.io/georgasa/tbsg-ai-rfp-assistant
DOCKER_TAG=latest




